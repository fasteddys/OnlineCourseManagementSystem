@model OnlineCourseManagementSystem.Web.ViewModels.Lectures.AllFilesByIdListViewModel
@{
    this.ViewData["Title"] = "All Files for the Lecture";
}

<h3>@this.ViewData["Title"]</h3>
@foreach (var file in this.Model.Files)
{
    <iframe src='https://view.officeapps.live.com/op/embed.aspx?src=@(file.RemoteUrl)' width='80%' height='565px' frameborder='0'> </iframe>

}