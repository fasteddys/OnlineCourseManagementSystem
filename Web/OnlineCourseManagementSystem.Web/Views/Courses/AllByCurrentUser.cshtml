@model OnlineCourseManagementSystem.Web.ViewModels.Courses.AllCoursesByUserListViewModel
@{

}

<style>
    .row-margin-05 {
        margin-top: 0.5em;
    }

    .row-margin-10 {
        margin-top: 1.0em;
    }

    .row-margin-20 {
        margin-top: 2.0em;
    }

    .row-margin-30 {
        margin-top: 3.0em;
    }
</style>

<head>
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.css" />
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />

    <script src="https://unpkg.com/swiper/swiper-bundle.js"></script>
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
</head>

<body class="bg-light">
    <div class="row shadow-lg p-3 mb-5 bg-info rounded">
        <div class="col-md-9">
            <div class="row">
                <div class="col-md-2">
                    <img src="@this.Model.CurrentUser.ProfileImageUrl" alt="studentProfileImage" height="100" width="100" style="border-radius: 50%;" />
                </div>
                <div class="col-md-10">
                    <h2 class="text-white">@(this.Model.CurrentUser.FirstName + ' ' + this.Model.CurrentUser.LastName)</h2>
                    <a class="text-white" style="text-decoration: underline;" href="#">View public profile</a>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <button class="btn btn-primary row-margin-20 text-white">
                Student
            </button>
        </div>
    </div>
    <div class="row">
        <div class="col-md-9">
            <div class="row">
                @foreach (var course in this.Model.Courses)
                {
                    <div class="col-md-4">
                        <div class="card bg-white shadow-lg p-3 mb-5 rounded">
                            <div class="row row-margin-10">
                                <div class="col-md-6">
                                    Lessons @course.CompletedLecturesCount of @course.CourseLecturesCount
                                </div>
                                <div class="col-md-6">
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar" style="width: 25%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="row-margin-10">
                                <img src="@course.CourseFileRemoteUrl" height="100" width="100" class="card-img-top" alt="courseImage">
                                <div class="card-body">
                                    <h5 class="card-title">
                                        <strong>@course.CourseName</strong>
                                    </h5>
                                    <a asp-action="ById" asp-route-id="@course.CourseId" class="btn btn-success row-margin-10">GO TO COURSE</a>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>

            <partial name="_MyCoursesPagingPartial" model="@this.Model" />
        </div>
        <div class="col-md-3">
            <div class="accordion shadow-lg p-3 mb-5 bg-white rounded" id="accordionExample">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="headingOne">
                        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <strong>My Account</strong>
                        </button>
                    </h2>
                    <div id="collapseOne" class="accordion-collapse collapse show" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
                        <div class="accordion-body">
                            <ul class="list-group">
                                <li href="#" class="list-group-item">
                                    Dashboard
                                </li>
                                <li href="#" class="list-group-item bg-info">
                                    My Courses
                                </li>
                                <li href="#" class="list-group-item">
                                    Profile
                                </li>
                                <li href="#" class="list-group-item">
                                    Messages
                                </li>
                                <li href="#" class="list-group-item">
                                    Logout
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row-margin-10">
