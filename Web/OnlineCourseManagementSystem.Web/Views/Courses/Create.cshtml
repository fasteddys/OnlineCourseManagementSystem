                <div asp-validation-summary="All" class="alert-danger"></div>

                <div class="form-group">
                    <label class="text-info" asp-for="Name"></label>
                    <input asp-for="Name" class="form-control" style="border-left: none; border-right: none; border-top: none;" />
                    <span asp-validation-for="Name" class="alert-danger"></span>
                </div>

                <div class="form-group">
                    <label class="text-dark" asp-for="Description"></label>
                    <textarea asp-for="Description" class="form-control" rows="12"></textarea>
                    <span asp-validation-for="Description" class="alert-danger"></span>
                </div>

                <input type="submit" value="Save" class="btn btn-primary float-end" />
            </form>
