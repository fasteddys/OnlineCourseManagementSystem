@model OnlineCourseManagementSystem.Web.ViewModels.Certificates.CertificateByUserIdAndCourseIdViewModel
@{
    this.ViewData["Title"] = $"My Certificate from {this.Model.CourseName}";
    int counter = 1;
}

<style>
    .row-margin-05 {
        margin-top: 0.5em;
    }

    .row-margin-10 {
        margin-top: 1.0em;
    }

    .row-margin-20 {
        margin-top: 2.0em;
    }

    .row-margin-30 {
        margin-top: 3.0em;
    }

   
</style>

<head>
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.css" />
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />

    <script src="https://unpkg.com/swiper/swiper-bundle.js"></script>
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
</head>

<body class="bg-light">
    <div class="row shadow-lg p-3 mb-5 bg-info rounded">
        <div class="col-md-9">
            <div class="row">
                <div class="col-md-2">
                    <img src="@this.Model.CurrentUser.ProfileImageUrl" alt="studentProfileImage" height="100" width="100" style="border-radius: 50%;" />
                </div>
                <div class="col-md-10">
                    <h2 class="text-white">@(this.Model.CurrentUser.FirstName + ' ' + this.Model.CurrentUser.LastName)</h2>
                    <a class="text-white" style="text-decoration: underline;" href="#">View public profile</a>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <button class="btn btn-primary row-margin-20 text-white">
                Student
            </button>
        </div>
    </div>
    <div class="row">
        <div class="col-md-8">
           <div id="printDiv" class="text-center text-white shadow-lg p-3 mb-5 rounded" style="background-color: orange; font-size: 15px; height: 100%; font-style: italic; border-radius: 20px; border: 10px solid brown;">
               <div>
                   <p>
                       <img src="https://tse1.mm.bing.net/th?id=OIP.B4uDqkFwAxtC3D6OYdlCVQHaF8&pid=Api&P=0&w=213&h=172" width="70" height="30" alt="logo" />
                       OCMS
                   </p>
               </div>
               <div class="row-margin-30">
                   <h1 style="font-size: 35px;">CERTIFICATE</h1>
                   <p>This certificate is issued by OCMS to acknowledge that</p>
                   <h2 style="font-size: 30px;">@(this.Model.UserFirstName + ' ' + this.Model.UserLastName)</h2>
                   <p>has successfully completed a course</p>
                   <h3 style="font-size: 23px;">@this.Model.CourseName - @this.Model.CourseCreatedOn.ToString("MMMM yyyy")</h3>
                   <p>with <strong class="text-white" style="font-size: 15px;">@this.Model.Grade.ToString("f2") / 6.00</strong></p>
               </div>
               <h3 style="text-decoration: underline;">Lectures:</h3>
               <div class="row-margin-30">

                   @foreach (var lecture in this.Model.Lectures)
                   {
                       <p style="font-size: 15px; font-style: italic;">@counter. @lecture.Title</p>
                       counter++;
                   }
               </div>

               <button id="print" class="btn btn-success row-margin-20">
                   <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-printer-fill" viewBox="0 0 16 16">
                       <path d="M5 1a2 2 0 0 0-2 2v1h10V3a2 2 0 0 0-2-2H5zm6 8H5a1 1 0 0 0-1 1v3a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1v-3a1 1 0 0 0-1-1z" />
                       <path d="M0 7a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v3a2 2 0 0 1-2 2h-1v-2a2 2 0 0 0-2-2H5a2 2 0 0 0-2 2v2H2a2 2 0 0 1-2-2V7zm2.5 1a.5.5 0 1 0 0-1 .5.5 0 0 0 0 1z" />
                   </svg>
                   PRINT
               </button>
           </div>
        </div>
        <div class="col-md-4">
            <div class="accordion shadow-lg p-3 mb-5 bg-white rounded" id="accordionExample">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="headingOne">
                        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <strong>My Account</strong>
                        </button>
                    </h2>
                    <div id="collapseOne" class="accordion-collapse collapse show" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
                        <div class="accordion-body">
                            <ul class="list-group">
                                <li class="list-group-item">
                                    <a asp-controller="Dashboard" asp-action="StudentDashboard">Dashboard</a>
                                </li>
                                <li class="list-group-item bg-info">
                                    My Courses
                                </li>
                                <li class="list-group-item">
                                    <a asp-controller="Users" asp-action="ManageAccountById">Profile</a>
                                </li>
                                <li class="list-group-item">
                                    <a asp-controller="Chats" asp-action="IndexChats">Messages</a>
                                </li>
                                <li class="list-group-item">
                                    <a asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@this.Url.Action("Index", "Home", new { area = string.Empty })">Logout</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
